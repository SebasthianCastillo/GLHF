{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "api/server.mjs",
    "api/model/Producto.js",
    "api/model/ProductDetail.js",
    "api/model/Category.js"
  ]
}
